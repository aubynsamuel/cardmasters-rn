{
  "expo": {
    "name": "cards",
    "slug": "cards",
    "version": "1.0.0",
    "orientation": "portrait", // set to default after fixing layout issues
    "icon": "./assets/images/icon.png",
    "scheme": "cardgame",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.aubynsamuel.cardmaster"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "325d7b98-c9fb-4585-b631-5f07450c9a52"
      }
    }
  }
}
