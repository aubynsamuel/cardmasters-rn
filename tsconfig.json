{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"],
      "@firebase/auth": ["./node_modules/@firebase/auth/dist/index.rn.d.ts"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "App.ts",
    "PlayedCardsComCardComponent.tsx",
    "server/server.js",
    "src/screens/GameClass.ts",
    "src/screens/CustomGameHook.ts",
    "src/screens/GameScreen.tsx"
  ]
}
